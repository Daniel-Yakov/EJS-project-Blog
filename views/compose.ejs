<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">

    <div class="mb-3">
        <label for="titleText" class="form-label">Title</label>
        <input type="text" class="form-control" name="titleText">
      </div>

      <div class="mb-3">
        <label for="postText" class="form-label">Post</label>
        <textarea class="form-control" name="postText" rows="3"></textarea>
      </div>

      <button class="btn btn-primary" type="submit">Publish</button>

</form>


<%- include("partials/footer"); -%>